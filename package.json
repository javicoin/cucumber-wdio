{
  "name": "cucumber-wdio",
  "version": "1.0.1",
  "description": "Run native automation for web, android and ios using cucumber wdio with page object pattern",
  "repository": {
    "type": "git",
    "url": "git://https://github.com/javicoin/cucumber-wdio"
  },
  "scripts": {
    "chrome": "wdio ./test/config/chrome.config.js",
    "android": "wdio ./test/config/android.config.js",
    "ios": "wdio ./test/config/ios.config.js",
    "xray": "node ./DummyReporter/dummyReporter.js",
    "lint": "eslint .",
    "allure_report_mobile": "./node_modules/.bin/allure generate --clean './test/reports/mobile/allure-results' && allure open",
    "allure_report_web": "./node_modules/.bin/allure generate --clean './test/reports/web/allure-results' && allure open"
  },
  "keywords": [
    "appium",
    "cucumber",
    "webdriverio",
    "wdio",
    "mobile automation"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@wdio/allure-reporter": "^6.11.0",
    "@wdio/appium-service": "^6.1.0",
    "@wdio/cli": "^6.1.12",
    "@wdio/cucumber-framework": "^6.11.1",
    "@wdio/local-runner": "^6.1.12",
    "allure-commandline": "^2.18.1",
    "eslint": "^7.3.0",
    "wdio-chromedriver-service": "^6.0.4"
  },
  "dependencies": {
    "@xray-app/xray-automation": "^0.2.0",
    "chai": "^4.2.0",
    "cucumber": "^6.0.5",
    "fix-esm": "^1.0.1",
    "multiple-cucumber-html-reporter": "^2.0.0",
    "wdio-cucumberjs-json-reporter": "^2.0.4"
  }
}
