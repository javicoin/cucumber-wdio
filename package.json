{
  "name": "appium-cucumber",
  "version": "1.0.0",
  "description": "Run native automation for android and ios using cucumber-wdio-allure with page object pattern",
  "scripts": {
    "android": "wdio ./config/android.config.js",
    "ios": "wdio ./config/ios.config.js",
    "open-allure": "./node_modules/.bin/allure generate --clean './allure-results' && allure open",
    "lint": "eslint ."
  },
  "keywords": [
    "Appium",
    "cucumber",
    "webdriverio",
    "wdio",
    "mobile automation"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@wdio/allure-reporter": "^6.11.0",
    "@wdio/appium-service": "^6.1.0",
    "@wdio/cli": "^6.1.12",
    "@wdio/cucumber-framework": "^6.1.13",
    "@wdio/local-runner": "^6.1.12",
    "@wdio/mocha-framework": "^6.1.8",
    "@wdio/spec-reporter": "^6.1.12",
    "eslint": "^7.3.0"
  },
  "dependencies": {
    "allure-commandline": "^2.17.2",
    "chai": "^4.2.0",
    "cucumber": "^6.0.5",
    "multiple-cucumber-html-reporter": "^1.16.2",
    "wdio-cucumberjs-json-reporter": "^2.0.4"
  }
}
